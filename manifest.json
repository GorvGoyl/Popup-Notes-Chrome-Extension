{
    "background": {
        "scripts": []
    },
    "manifest_version": 2,
    "name": "Popup Notes",
    "description": "Take quick notes while you surf. What could be more simple?",
    "version": "1.0",
    "icons": {
        "128": "images/icon.png",
        "16": "images/icon.png",
        "48": "images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon.png",
            "38": "images/icon.png"
        },
        "default_popup": "popup.html",
        "default_title": "Popup Notes!"
    },
    "offline_enabled": true,
    "permissions": [
        "storage", "bookmarks", "contextMenus"
    ]
}
